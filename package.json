{
  "name": "etcd-explorer",
  "displayName": "etcd-explorer",
  "description": "",
  "version": "0.0.1",
  "publisher": "mkv",
  "icon": "resources/etcd.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/4mkv/vscode-etcd-explorer"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:etcd3view",
    "onView:etcd2view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "EtcdExplorer",
      "properties": {
        "etcd-explorer.etcd_host": {
          "type": "string",
          "default": "",
          "description": "hostname or ip address of etcd host and port [host:port]"
        },
        "etcd-explorer.max_keys_per_level": {
          "type": "number",
          "default": 20,
          "description": "Max number of items to show per level"
        }
      }
    },
    "commands": [
      {
        "command": "etcd3view.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "etcd2view.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "etcd3view.deleteEntry",
        "title": "Delete"
      },
      {
        "command": "etcd2view.deleteEntry",
        "title": "Delete"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "etcd3view.refreshEntry",
          "when": "view == etcd3view",
          "group": "navigation"
        },
        {
          "command": "etcd2view.refreshEntry",
          "when": "view == etcd2view",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "etcd3view.deleteEntry",
          "when": "view == etcd3view"
        },
        {
          "command": "etcd2view.deleteEntry",
          "when": "view == etcd2view"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "etcd-explorer",
          "title": "ETCD",
          "icon": "resources/etcd.svg"
        }
      ]
    },
    "views": {
      "etcd-explorer": [
        {
          "id": "etcd3view",
          "name": "ETCD3 EXPLORER"
        },
        {
          "id": "etcd2view",
          "name": "ETCD2 EXPLORER"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "electron": "^7.1.14",
    "electron-rebuild": "^1.10.1",
    "grpc": "^1.23.3",
    "eslint": "^6.8.0",
    "etcd3": "^0.2.13",
    "node-etcd": "^7.0.0",
    "hashmap": "^2.4.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "hashmap": "^2.4.0",
    "grpc": "^1.23.3",
    "etcd3": "^0.2.13",
    "node-etcd": "^7.0.0"
  }
}